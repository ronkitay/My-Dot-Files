function rgb()
{
    echo -e "\033]6;1;bg;red;brightness;$1\a\033]6;1;bg;green;brightness;$2\a\033]6;1;bg;blue;brightness;$3\a"
}

# iTerm stuff
function tabrand() {
    let red="$RANDOM % 255"
    let green="$RANDOM % 255" 
    let blue="$RANDOM % 255"
    rgb $red $green $blue
}

alias tabnormal='echo -e "\033]6;1;bg;*;default\a"'
alias tabgreen='rgb 115 199 88'
alias tabblue='rgb 30 169 214'
alias tabred='rgb 214 75 30'

alias 1='tabrand'